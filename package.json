{
  "name": "rnb",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://ethanneff.github.io/react-native-boilerplate",
  "devDependencies": {
    "@commitlint/cli": "^4.2.2",
    "@commitlint/config-angular": "^4.2.1",
    "coveralls": "^3.0.0",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "jest-expo": "^22.0.0",
    "lint-staged": "^4.3.0",
    "prettier": "^1.7.4",
    "react-devtools": "^2.5.2",
    "react-native-scripts": "1.6.0",
    "react-native-web": "^0.1.13",
    "react-scripts": "^1.0.16",
    "react-test-renderer": "16.0.0",
    "remote-redux-devtools": "^0.5.12"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "web": "react-scripts start",
    "build": "react-scripts build",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "predeploy": "npm run web:build",
    "deploy": "gh-pages -d build",
    "test":
      "node node_modules/.bin/jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "testing": "node node_modules/jest/bin/jest.js --watch"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": ["prettier --write", "git add"]
  },
  "commitlint": {
    "extends": ["@commitlint/config-angular"]
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "expo": "^22.0.0",
    "react": "16.0.0",
    "react-dom": "^16.0.0",
    "react-native": "^0.50.0"
  }
}
